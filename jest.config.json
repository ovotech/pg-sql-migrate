{
  "transform": {
    "^.+\\.tsx?$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
  "moduleFileExtensions": ["ts", "tsx", "js", "jsx", "tsx", "json", "node"],
  "testURL": "http://localhost/",
  "testEnvironment": "node",
  "modulePathIgnorePatterns": ["<rootDir>/dist/"]
}
